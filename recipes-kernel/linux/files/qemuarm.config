# versatilepb machine needs ARM EABI support
CONFIG_AEABI=y

# runqemu assumes that ext4 is the default filesystem
CONFIG_EXT4_FS=y
# runqemu uses virtio disk (/dev/vda) as rootfs by default
CONFIG_PCI=y
CONFIG_VIRTIO_PCI=y
CONFIG_VIRTIO_BLK=y
# runqemu uses virtio network interface
CONFIG_VIRTIO_NET=y

# tiny-init depends on devtmpfs
CONFIG_DEVTMPFS=y
CONFIG_DEVTMPFS_MOUNT=n
